<% provide(:title, 'New Park Form') %>

<%= form_for @new_park do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control', placeholder: 'e.g. Yosemite' %>
  </div>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_field :description, class: 'form-control', placeholder: 'e.g. Lorem Ipsum' %>
  </div>
  <div class="form-group">
    <%= f.label :picture %>
    <%= f.text_field :picture, class: 'form-control', placeholder: 'e.g. url of the picture' %>
  </div>
  <%= f.submit class: 'btn btn-primary' %>
<% end %>

